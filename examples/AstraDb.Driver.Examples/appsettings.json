{
  "Astra": {
    "Driver": {
      "SecureConnectBundlePath": "%USERPROFILE%\\.astradb\\secure-connect-dev.zip",
      "Token": "AstraCS:your_token_here",
      "Keyspace": "dev_ks"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ]
  }
}
